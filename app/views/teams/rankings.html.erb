<% provide(:title , "Team Rankings") %>
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/foundation/6.4.1/css/foundation.min.css">
<br><hr>
<div class='large-9 medium-10 small-12 large-centered medium-centered columns'>
	<ul class="tabs" data-tabs id="example-tabs">
	  <li class="tabs-title is-active"><a href="#panel1" aria-selected="true" style="font-size: 50%; color: black;">Baseball</a></li>
	  <li class="tabs-title"><a data-tabs-target="panel2" href="#panel2" style="font-size: 50%; color: black;">Basketball</a></li>
	  <li class="tabs-title"><a data-tabs-target="panel3" href="#panel3" style="font-size: 50%; color: black;">Football</a></li>
	  <li class="tabs-title"><a data-tabs-target="panel4" href="#panel4" style="font-size: 50%; color: black;">Hockey</a></li>
	  <li class="tabs-title"><a data-tabs-target="panel5" href="#panel5" style="font-size: 50%; color: black;">Soccer</a></li>


	</ul>
	<div class="tabs-content" data-tabs-content="example-tabs">
	  <div class="tabs-panel is-active" id="panel1">
	  		<ul class="tabs" data-tabs id="example-tabs">
		  <li class="tabs-title is-active" style="width: 10%"><a href="#baseball1" aria-selected="true">Team Rating</a></li>
		  <li class="tabs-title" style="width: 10%"><a data-tabs-target="baseball2" href="#baseball2">Stadium Rating</a></li>
		  <li class="tabs-title" style="width: 10%"><a data-tabs-target="baseball3" href="#baseball3">Coach Rating</a></li>
		 <hr>
		  <div class="tabs-content" data-tabs-content="example-tabs">

		  	<div class="tabs-panel is-active" id="baseball1">
			    <% @baseball.sort_by{|team| team.team_score}.reverse.each do |team| %>
				    <div class='large-4 medium-6 small-12 columns' style='margin-bottom:10px;'>
				    <!-- <%= render team %> -->
				    <%= link_to(image_tag(team.thumb, :style => 'width:65px;height:65px;'), team) %>
				    <p style='display:inline;'><%=link_to team.name, team %> - <%= team.score_percentage("team") %>% </p>

				    </div>
			    <% end %>
			  </div>

			  <div class="tabs-panel" id="baseball2">
			    <% @baseball.sort_by{|team| team.stadium_score}.reverse.each do |team| %>
				    <div class='large-4 medium-6 small-12 columns' style='margin-bottom:50px;'>
				    <!-- <%= render team %> -->
				    <%= link_to(image_tag(team.thumb, :style => 'width:65px;height:65px;'), team) %>
				    <p style='display:inline;'><%=link_to team.stadium, team %> - <%= team.score_percentage("stadium") %>% </p>

				    </div>
			    <% end %>
			  </div>

			  <div class="tabs-panel" id="baseball3">
			    <% @baseball.sort_by{|team| team.coach_score}.reverse.each do |team| %>
				    <div class='large-4 medium-6 small-12 columns' style='margin-bottom:50px;'>
				    <!-- <%= render team %> -->
				    <%= link_to(image_tag(team.thumb, :style => 'width:65px;height:65px;'), team) %>
				    <p style='display:inline;'><%=link_to team.coach, team %> - <%= team.score_percentage("coach") %>% </p>

				    </div>
			    <% end %>
			  </div>

			</div>
	  </div>

	  <div class="tabs-panel" id="panel2">
	  	<div class="tabs-panel is-active" id="panel1">
